	pages = {711--33}
}

@article{kroch_reflexes_1989,
	title = {Reflexes of {Grammar} in {Patterns} of {Language} {Change}},
	volume = {1},
	issn = {0954-3945},
	doi = {10.1017/S0954394500000168},
	abstract = {Evidence from various linguistic changes is presented to support the view that the time course of such change is significantly constrained by the grammar of the language involved. It is argued that where one grammatical option replaces another across a set of linguistic contexts, the rate of replacement can be shown to be the same in all those contexts, regardless of differences in the degree to which they may favor the new form. It is argued that the set of contexts that changes together is not usually defined by an obvious surface property, but by a shared syntactic structure. A detailed analysis is presented of the rise of the periphrastic auxiliary do in Late Middle English \& Early Modern English. Other English data also support the constant rate hypothesis. It is shown that the fact that change arises first in contexts that favor the change functionally allows quantitative study of the causation of change. These causes of change should be sought at abstract levels of structure, particularly where entire grammatical subsystems may be involved. 12 Tables, 8 Figures, 57 References. B. Annesser Murray},
	language = {English},
	number = {3},
	journal = {Language Variation and Change},
	author = {Kroch, Anthony S.},
	year = {1989},
	keywords = {English Language (Middle), French Language (Middle), Language Change, Periphrastic Tense, Portuguese Language, Syntax},
	pages = {199--244}
}

